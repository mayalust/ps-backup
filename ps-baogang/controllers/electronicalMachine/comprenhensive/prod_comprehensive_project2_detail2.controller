                    if(domains && domains.motorInfo && domains.motorInfo.motorType){//设备信息
                        if(domains.motorInfo.motorType == 'AC_MOTOR'){
                            domains.motorInfo.motorType = '交流电机';
                        }else if(domains.motorInfo.motorType == 'DC_MOTOR'){
                            domains.motorInfo.motorType = '直流电机';
                        }else if(domains.motorInfo.motorType == 'TRANSFORMER'){
