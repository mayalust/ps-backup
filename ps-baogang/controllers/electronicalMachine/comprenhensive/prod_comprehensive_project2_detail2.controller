                            'motDepartment': '运输部',
                            'siliconSteelDepartment': '硅钢部',
                            'baori': '宝日',
                            'bwtcDepartment': '宝武碳材',
                            'elseDepartment': '其他'
                        };
                        if(objDepartment.hasOwnProperty(domains.motorInfo.department)){
                            domains.motorInfo.department = objDepartment[domains.motorInfo.department];
                        }else {
