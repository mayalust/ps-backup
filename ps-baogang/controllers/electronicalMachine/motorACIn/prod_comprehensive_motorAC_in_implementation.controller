<script>
    import { dateparser } from "ps-ultility";
    export default function( scope,timeout,ajax,$state,registerService,cms2,growl,commonMethodService,psLoading){
        var target = commonMethodService();
        scope.mode = target.getValue("mode");
        if(scope.mode == "resume"){
            scope.maintainObj = target.getValue("resumeObj");
        }else {
            scope.maintainObj = target.getValue("maintainObj");
        }
        if(scope.maintainObj.status == 200){
            scope.mode = "resume";
        }
        scope.maintainObj = target.getValue("maintainObj");
        scope.submitFn = function(){
            let promise = cms2.validate({
                "maintainObj.statorInsulationFinishedProduct" : "定子绝缘不可为空",
                "maintainObj.rotorInsulationFinishedProduct" : "转子绝缘不可为空",
                "maintainObj.measuringToolNumberFinishedProduct" : "成品送试前量具编号不可为空",
                "maintainObj.catcherExFactory" : "出厂接手不可为空",
                "maintainObj.terminalBlock" : "出厂接线盒不可为空",
                "maintainObj.junctionBoxCover" : "出厂接线盒盖不可为空",
            }, scope);
            promise.then(() => {
                if( typeof (scope && scope.maintainObj) != "undefined"){
                    function getFiles(data) {
                        let keys = Object.keys(data);
                        let values = Object.values(data);
                        let temp = [];
                        let ret = [];
                        keys.forEach((key, index) => {
                            let obj = {};
                            obj.key = key;
                            obj.files = values[index];
                            temp.push(obj);
                        });

                        let index = 0;
                        temp.forEach(item => {
                            if (item.files.length > 0) {
                                item.files.forEach(file => {
                                    ret.push({
                                        name: file.name,
                                        key: item.key,
                                        file: file,
                                        index: index
                                    });
                                    index ++ ;
                                });
                            }
                        });

                        return ret;
                    }
                    function setValueEmpty(data) {
                        for(let key in data) {
                            scope.maintainObj[key] = [];
                        }
                    }
                    let formData = new FormData();
                    let obj = {
                        'attachmentListLead':  scope.maintainObj.attachmentListLead || [],
                        'attachmentListRotor':  scope.maintainObj.attachmentListRotor || [],
                        'attachmentListRotorCore':  scope.maintainObj.attachmentListRotorCore || [],
                        'attachmentListBrushHolder':  scope.maintainObj.attachmentListBrushHolder || [],
                        'attachmentListbearingType':  scope.maintainObj.attachmentListbearingType || [],
                        'attachmentListBearingChamber':  scope.maintainObj.attachmentListBearingChamber || [],
                        'attachmentListbearingFile':  scope.maintainObj.attachmentListbearingFile || [],
                        'attachmentListEndcover':  scope.maintainObj.attachmentListEndcover || [],
                        'attachmentListDynamicBalance':  scope.maintainObj.attachmentListDynamicBalance || [],
                        'attachmentListPart':  scope.maintainObj.attachmentListPart || [],
                        'attachmentListFinishedProduct':  scope.maintainObj.attachmentListFinishedProduct || [],
                        'attachmentListExFactory':  scope.maintainObj.attachmentListExFactory || [],

                    };
                    let files = getFiles(obj);
                    setValueEmpty(obj);
                    formData.append("resourceId",2);
                    if(files.length >0){
                        files.forEach((v,i) => {
                            formData.append(`file${v.index}`,v.file);
                        })
                    }
                    if(files.length !== 0){
                        psLoading.showLoadingTip();
                        ajax.upload("resourceFileUIService.uploadResourceFile",formData).then(function (s) {
                            if(s.code == "0"){
                                psLoading.closeLoading();
                                let obj = {};
                                if(isArray(s.data)){
                                    s.data.forEach((v,i)=>{
                                        obj = {};
                                        obj["fileName"] = files[i].name;
                                        obj["filePath"] = v.qualifiedName;
                                        scope.maintainObj[files[i].key].push(obj);
                                    })
                                }else {
                                    obj["fileName"] = files[0].name;
                                    obj["filePath"] = s.data.qualifiedName;
                                    scope.maintainObj[files[0].key].push(obj);
                                }
                                return cms2.success( scope);
                            }
                        }).then(function (scope) {
                            psLoading.showLoading();
                            ajax.post("motorRepairUIService.saveMaintenanceImplementationAC",scope.maintainObj).then( d => {
                                if(d != null){
                                    psLoading.closeLoading();
                                    growl.info("设备检修实施成功！");
                                    // cms2.navigateTo(3,0);
                                }
                            })
                        })
                    }else {
                        psLoading.showLoading();
                        ajax.post("motorRepairUIService.saveMaintenanceImplementationAC",scope.maintainObj).then( d => {
                            if(d != null){
                                psLoading.closeLoading();
                                growl.info("设备检修实施成功！");
                                // cms2.navigateTo(3,0);
                            }
                        })
                    }
                }

            }).catch(e => {
                growl.error( e );
            })
        }
        scope.close = function(){
            cms2.navigateTo(3,0);
        }
        scope.sub = function () {
            psLoading.showLoading();
            ajax.post("motorRepairUIService.synthesizeProcess",[scope.maintainObj.implementOrderNo]).then(d => {
                if(d != null){
                    psLoading.closeLoading();
                    growl.info("综合处理成功");
                    cms2.navigateTo(3);
                }
            })
        }
        scope.back = function () {
            cms2.navigateTo(3);
        }
    }
</script>
