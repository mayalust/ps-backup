<script>
    import { dateparser } from "ps-ultility";
    export default function( scope,timeout,ajax,$state,registerService,cms2,growl,commonMethodService,psLoading){
        var target = commonMethodService();
        scope.mode = target.getValue('mode');
        scope.maintainObjNo = target.getValue("maintainObj").implementOrderNo;
        scope.currentRoleID = target.getCurrentUser().currentRoleID;
        ajax.post("motorRepairUIService.findOneDismantlingAndInspectAC",[scope.maintainObjNo]).then(d => {
            if(d!=null){
                scope.maintainObj = d;
            }
        });
        scope.editResume = function (){
          scope.mode = 'superAdmin';
        };
        scope.disableFn = function (mode,currentRoleID){
          return mode === 'resume'
              || (currentRoleID == 59846674150228 && mode != 'superAdmin')
              || (currentRoleID != 70153973788970 && currentRoleID != 59846674150228);
        }
        scope.submitFn = function(){
            let promise = cms2.validate({
                 "maintainObj.statorInsulation1" : " 定子绝缘1不可为空",
                 "maintainObj.statorA1" : "定子三相电阻a1不可为空",
                 "maintainObj.statorB1" : "定子三相电阻b1不可为空",
                 "maintainObj.statorC1" : "定子三相电阻c1不可为空",
                 "maintainObj.preDismantlingMeasuringToolNumber" : "量具编号不可为空",
                 "maintainObj.bearingRoomLoadSide" : "负荷侧轴承室尺寸不可为空",
                 "maintainObj.bearingRoomNonLoadSide" : "非负荷侧轴承室尺寸不可为空",
                 "maintainObj.bearingRoomMeasuringToolNumber" : "轴承室量器具不可为空",
                 "maintainObj.bearingRetainerLoadSide" : "负荷侧轴承档不可为空",
                 "maintainObj.bearingRetainerNonLoadSide" : "非负荷侧轴承档不可为空",
                 "maintainObj.bearingRetainerMeasuringToolNumber" : "轴承档量具编号不可为空",
            }, scope);
            promise.then(() => {
                if( typeof (scope && scope.maintainObj) != "undefined"){
                    scope.maintainObj.implementOrderNo = scope.maintainObjNo;//根据implementOrderNo查询
                    function getFiles(data) {
                        let keys = Object.keys(data);
                        let values = Object.values(data);
                        let temp = [];
                        let ret = [];
                        keys.forEach((key, index) => {
                            let obj = {};
                            obj.key = key;
                            obj.files = values[index];
                            temp.push(obj);
                        });

                        let index = 0;
                        temp.forEach(item => {
                            if (item.files.length > 0) {
                                item.files.forEach(file => {
                                    ret.push({
                                        name: file.name,
                                        key: item.key,
                                        file: file,
                                        index: index
                                    });
                                    index ++ ;
                                });
                            }
                        });

                        return ret;
                    }
                    function setValueEmpty(data) {
                        for(let key in data) {
                            scope.maintainObj[key] = [];
                        }
                    }
                    let formData = new FormData();
                    let hasFileObj = {};
                    hasFileObj.attachmentListPreDismantling = scope.maintainObj.attachmentListPreDismantling&&scope.maintainObj.attachmentListPreDismantling.map(v=>{
                        if(v.hasOwnProperty('filePath'))
                        {return v;}
                    }) || [];
                    hasFileObj.attachmentListStatorAndRotor = scope.maintainObj.attachmentListStatorAndRotor&&scope.maintainObj.attachmentListStatorAndRotor.map(v=>{
                        if(v.hasOwnProperty('filePath'))
                        {return v;}
                    }) || [];
                    hasFileObj.attachmentListMechanicalPart = scope.maintainObj.attachmentListMechanicalPart&&scope.maintainObj.attachmentListMechanicalPart.map(v=>{
                        if(v.hasOwnProperty('filePath'))
                        {return v;}
                    }) || [];
                    hasFileObj.attachmentListOther = scope.maintainObj.attachmentListOther&&scope.maintainObj.attachmentListOther.map(v=>{
                        if(v.hasOwnProperty('filePath'))
                        {return v;}
                    }) || [];
                    let obj = {
                        'attachmentListPreDismantling':  scope.maintainObj.attachmentListPreDismantling&&scope.maintainObj.attachmentListPreDismantling.filter(v=>!v.hasOwnProperty('filePath')) || [],
                        'attachmentListStatorAndRotor':  scope.maintainObj.attachmentListStatorAndRotor&&scope.maintainObj.attachmentListStatorAndRotor.filter(v=>!v.hasOwnProperty('filePath')) || [],
                        'attachmentListMechanicalPart':  scope.maintainObj.attachmentListMechanicalPart&&scope.maintainObj.attachmentListMechanicalPart.filter(v=>!v.hasOwnProperty('filePath')) || [],
                                 'attachmentListOther':  scope.maintainObj.attachmentListOther&&scope.maintainObj.attachmentListOther.filter(v=>!v.hasOwnProperty('filePath')) || [],
                    };
                    let files = getFiles(obj);
                    setValueEmpty(obj);
                    formData.append("resourceId",2);
                    if(files.length >0){
                        files.forEach((v,i) => {
                            formData.append(`file${v.index}`,v.file);
                        })
                    }
                    if(files.length !== 0){
                        psLoading.showLoadingTip();
                        ajax.upload("resourceFileUIService.uploadResourceFile",formData).then(function (s) {
                            if(s.code == "0"){
                                psLoading.closeLoading();
                                let obj = {},sArr = s.data || [];
                                if(isArray(s.data)){
                                    s.data.forEach((v,i)=>{
                                        obj = {};
                                        obj["fileName"] = files[i].name;
                                        // obj["filePath"] = v.qualifiedName;
                                        obj["filePath"] = sArr.find((val,k) => val.qualifiedName.split('-')[1] === files[i].name ).qualifiedName;
                                        scope.maintainObj[files[i].key].push(obj);
                                    })
                                }else {
                                    obj["fileName"] = files[0].name;
                                    obj["filePath"] = s.data.qualifiedName;
                                    scope.maintainObj[files[0].key].push(obj);
                                }
                                return cms2.success( scope);
                            }
                        }).then(function (scope) {
                            psLoading.showLoading();
                            scope.maintainObj.attachmentListPreDismantling = scope.maintainObj.attachmentListPreDismantling.concat(hasFileObj.attachmentListPreDismantling&&hasFileObj.attachmentListPreDismantling.filter(v=>v));
                            scope.maintainObj.attachmentListStatorAndRotor = scope.maintainObj.attachmentListStatorAndRotor.concat(hasFileObj.attachmentListStatorAndRotor&&hasFileObj.attachmentListStatorAndRotor.filter(v=>v));
                            scope.maintainObj.attachmentListMechanicalPart = scope.maintainObj.attachmentListMechanicalPart.concat(hasFileObj.attachmentListMechanicalPart&&hasFileObj.attachmentListMechanicalPart.filter(v=>v));
                            scope.maintainObj.attachmentListOther = scope.maintainObj.attachmentListOther.concat(hasFileObj.attachmentListOther&&hasFileObj.attachmentListOther.filter(v=>v));
                            ajax.post("motorRepairUIService.saveDismantlingAndInspectAC",scope.maintainObj).then( d => {
                                if(d != null){
                                    psLoading.closeLoading();
                                    growl.info("设备拆检成功！");
                                    // cms2.navigateTo(3,0);
                                }
                            })
                        })
                    }else {
                        psLoading.showLoading();
                        scope.maintainObj.attachmentListPreDismantling = scope.maintainObj.attachmentListPreDismantling.filter(v=>!v.hasOwnProperty('filePath')).concat(hasFileObj.attachmentListPreDismantling&&hasFileObj.attachmentListPreDismantling.filter(v=>v));
                        scope.maintainObj.attachmentListStatorAndRotor = scope.maintainObj.attachmentListStatorAndRotor.filter(v=>!v.hasOwnProperty('filePath')).concat(hasFileObj.attachmentListStatorAndRotor&&hasFileObj.attachmentListStatorAndRotor.filter(v=>v));
                        scope.maintainObj.attachmentListMechanicalPart = scope.maintainObj.attachmentListMechanicalPart.filter(v=>!v.hasOwnProperty('filePath')).concat(hasFileObj.attachmentListMechanicalPart&&hasFileObj.attachmentListMechanicalPart.filter(v=>v));
                        scope.maintainObj.attachmentListOther = scope.maintainObj.attachmentListOther.filter(v=>!v.hasOwnProperty('filePath')).concat(hasFileObj.attachmentListOther&&hasFileObj.attachmentListOther.filter(v=>v));
                        ajax.post("motorRepairUIService.saveDismantlingAndInspectAC",scope.maintainObj).then( d => {
                            if(d != null){
                                psLoading.closeLoading();
                                growl.info("设备拆检成功！");
                            }
                        })
                    }
                }

            }).catch(e => {
                growl.warning( e);
            })
        }
        scope.close = function(){
            cms2.navigateTo(3,0);
        }
        scope.sub = function () {
            psLoading.showLoading();
            ajax.post("motorRepairUIService.synthesizeProcess",[scope.maintainObj.implementOrderNo]).then(d => {
                if(d != null){
                    psLoading.closeLoading();
                    growl.info("综合处理成功");
                    cms2.navigateTo(3);
                }
            })
        }
        scope.back = function () {
           // cms2.navigateTo(3);
            cms2.refresh("prod_comprehensive_project_list_mangage");
        }
    }
</script>
